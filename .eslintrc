{
  "extends": "eslint-config-egg",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "globals": {
    "window": true,
    "document": true,
    "require": true,
    "module": true,
    "console": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "new-cap": "off",
    "newline-after-var": "off",
    "no-invalid-this": "off",
    "quotes": [
      "warn",
      "single"
    ],
    "prefer-template": "off",
    "prefer-arrow-callback": "off",
    "quote-props": "off",
    "guard-for-in": "off",
    "no-console": "off",
    "no-undefined": "off",
    "array-bracket-spacing": "off",
    "no-unused-expressions": "off",
    "no-useless-return": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "lines-around-comment": "off",
    "no-useless-escape":"off",
    "consistent-return":"off",
    "require-jsdoc": "off",
    "semi-spacing":"off",
    "func-style": [
      "warn",
      "expression"
    ],
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}
